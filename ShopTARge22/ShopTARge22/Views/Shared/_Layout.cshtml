<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - ShopTARge22</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/ShopTARge22.styles.css" asp-append-version="true" />
</head>
<div id="wrapper">
<body>
    
    <header id="header" class="alt">
            <a asp-area="" asp-controller="Home" asp-action="Index"><span class="logo"><img src="../../images/logo.svg" alt=""></span></a>
            <h1>ShopTARge22</h1>
@*             <partial name="_LoginPartial" />
 *@
            <ul class="navbar-nav ml-auto">
                <li class="nav-item">
                    <a asp-controller="Accounts" asp-action="Register" class="nav-link m-1" style="color:black">Register</a>
                </li>
                <li class="nav-item text-dark">
                    <a asp-controller="Accounts" asp-action="Login" class="nav-link m-1" style="color:black">Login</a>
                </li>
            </ul>
        </header>
        <nav class="navbar navbar-expand-sm navbar-toggleable-sm">
            <div class="container-fluid">
               
                <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target=".navbar-collapse" aria-controls="navbarSupportedContent"
                        aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                </button>
                <div class="navbar-collapse collapse d-sm-inline-flex justify-content-between">
                    <nav id="nav" class>
                    <ul>
                        <li>
                            <a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="Index">Home</a>
                        </li>
                        <li>
                            <a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
                        </li>
                        <li>
                            <a class="nav-link text-dark" asp-area="" asp-controller="Spaceships" asp-action="Index">Spaceships</a>
                        </li>
                        <li>
                            <a class="nav-link text-dark" asp-area="" asp-controller="RealEstates" asp-action="Index">RealEstates</a>
                        </li>
                        <li>
                            <a class="nav-link text-dark" asp-area="" asp-controller="Kindergartens" asp-action="Index">Kindergarten</a> 
                        </li>
                        <li>
                            <a class="nav-link text-dark" asp-area="" asp-controller="WeatherForecasts" asp-action="Index">Weather Forecasts</a>
                        </li>
                        <li>
                            <a class="nav-link text-dark" asp-area="" asp-controller="AccuWeathers" asp-action="Index">AccuWeather</a>
                        </li>
                        <li>
                            <a class="nav-link text-dark" asp-area="" asp-controller="ChuckNorris" asp-action="Chuck">Chuck Norris</a>
                        </li>
                        <li>
                                <a class="nav-link text-dark" asp-area="" asp-controller="Cocktails" asp-action="Index">Cocktails</a>
                        </li>
                        <li>
                                <a class="nav-link text-dark" asp-area="" asp-controller="Emails" asp-action="Index">Emails</a>
                        </li>

                    </ul>

                        
                    </nav>
                </div>
            </div>

        </nav>
    <div class="container">
        <main role="main" class="pb-3">
            <div id="main">
                <section class="main">

                @RenderBody()
                </section>
            </div>
        </main>
    </div>

    <footer class="border-top footer text-muted" id="footer">
        <div class="container">
            &copy; 2023 - ShopTARge22 - <a asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
        </div>
    </footer>

    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)



</body>
</div>
</html>
